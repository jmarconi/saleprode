{% extends 'JmSaleBundle:Default:admin-base.html.twig' %}
{% block body %}
<h1>Torneo list</h1>

<table class="records_list table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('admin_torneo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.nombre }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('admin_torneo_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admin_torneo_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('admin_torneo_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
{% endblock %}